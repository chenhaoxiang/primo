<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wiki.primo.generator.primogeneratormocktestdemo.mapper.UserTemplateMapper">
	<!--开启二级缓存-->
	<cache/>

	<resultMap id="BaseResultMap" type="wiki.primo.generator.primogeneratormocktestdemo.entity.UserTemplate">
		<id column="id" property="id" />
		<result column="ext" property="ext" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
		<result column="past_time" property="pastTime" />
		<result column="user_id" property="userId" />
		<result column="template_code" property="templateCode" />
		<result column="domain" property="domain" />
		<result column="password" property="password" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        `id`, `ext`, `create_time` AS `createTime`, `update_time` AS `updateTime`, `past_time` AS `pastTime`, `user_id` AS `userId`, `template_code` AS `templateCode`, `domain`, `password`
    </sql>
</mapper>
