<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wiki.primo.generator.mybatis.plus.springbootdemo.mapper.UserMapper">
	<!--开启二级缓存-->
	<cache/>

	<resultMap id="BaseResultMap" type="wiki.primo.generator.mybatis.plus.springbootdemo.entity.User">
		<id column="id" property="id" />
		<result column="name" property="name" />
		<result column="password" property="password" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
		<result column="past_time" property="pastTime" />
		<result column="sex" property="sex" />
		<result column="phone" property="phone" />
		<result column="email" property="email" />
		<result column="describe" property="describe" />
		<result column="role" property="role" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        `id`, `name`, `password`, `create_time` AS `createTime`, `update_time` AS `updateTime`, `past_time` AS `pastTime`, `sex`, `phone`, `email`, `describe`, `role`
    </sql>
</mapper>
